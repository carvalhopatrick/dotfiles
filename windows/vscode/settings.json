    "editor.tokenColorCustomizations": {
        "[Vue Edit Twski]": {
            /* padr√µes:
                types           #ff2c6bf1 (rosa tutti forte)
                funcoes         #09cbdd (azul claro forte)
                foreground      #e6e6e6 (branco)
                strings         #00abc1da (azul escuro)
                sinais          #ffcc95e7 (laranja claro)

                */
            "types": "#f57fa2c2"  ,  
            "comments": "#757474",
            "functions": "#4ca1a8",     // azul escurinho #4ca1a8    azul claro dhr #a1d7da
            "keywords": "#ffcc95e7",
            "textMateRules": [
                // deixa if, for, etc em ITALICO --- ignorar cor definida aqui, ela nao vale
                {
                    "scope": "keyword.control",
                    "settings": {
                        //"foreground": "#89DDFF",
                        "fontStyle": "italic"
                    }
                },
                // cor de parenteses, brackets, etc
                {
                    "scope": "punctuation, constant.other.symbol",
                    "settings": {
                        "foreground": "#ffd2a1"
                    }
                },
                // cor de strings
                {
                    "name": "string between quotes and template literals",
                    "scope": "string.quoted.double, string.template",
                    "settings": {
                        "foreground": "#45b49ce0"
                    }
                },
                // ???
                {
                    "name": "variable object",
                    "scope": "variable.other.object.property, variable.other.property, meta.template.expression, support.variable.property, variable.language.super, support.class.component.js, entity.name.tag.js",
                    "settings": {
                        "foreground": "#dd93b2c5"
                    }
                },
            ]
        },

        "[Community Material Theme Ocean High Contrast]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment",
                        "punctuation.definition.comment",
                        "string.quoted.docstring"
                    ],
                    "settings": {
                        "foreground": "#4b5d46",    // stock: #464B5D
                        "fontStyle": "italic"
                    }
                },
            ]
        }
        
    },
